{
  "name": "@mdotasia/components",
  "version": "0.0.7",
  "description": "Reusable React components.",
  "homepage": "https://github.com/mdotasia/components",
  "bugs": "https://github.com/mdotasia/components/issues",
  "author": {
    "name": "Thilo Maier",
    "email": "git@maier.asia"
  },
  "repository": "git@github.com:mdotasia/components.git",
  "license": "MIT",
  "keywords": ["react", "components", "styleguide"],
  "main": "dist/index.js",
  "files": ["dist"],
  "scripts": {
    "start": "catalog start",
    "prebuild": "rimraf ./dist",
    "build":
      "cross-env NODE_ENV=production babel ./src -d ./dist --ignore test.js",
    "build:catalog": "catalog build",
    "deploy": "gh-pages -d catalog/build",
    "lint": "eslint .",
    "precommit": "pretty-quick --staged",
    "prepublish": "yarn run build",
    "release": "release",
    "postrelease": "yarn run build:catalog && yarn run deploy",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "rebass": "^1.0.4",
    "styled-components": "^2.1.2",
    "styled-system": "^1.0.0-13"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.5.0",
    "catalog": "^3.0.0-rc.4",
    "cross-env": "^5.1.3",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.4.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.0",
    "release": "^3.1.5",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": ["react-app"],
    "plugins": ["styled-components"]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
