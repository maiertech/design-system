{
  "name": "@maiertech/components",
  "description": "React component library for @maiertech websites.",
  "version": "0.19.0",
  "author": "Thilo Maier <git@maier.asia>",
  "license": "MIT",
  "homepage": "https://github.com/maiertech/design-system/tree/master/packages/components",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maiertech/design-system.git",
    "directory": "packages/components"
  },
  "keywords": [
    "react",
    "rebass"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "start-storybook -p 8000 -c config/storybook",
    "storybook-build": "rm -rf ./storybook-static && build-storybook -c config/storybook",
    "prepare": "microbundle --format modern,cjs --jsx React.createElement",
    "watch": "microbundle watch --format modern,cjs --jsx React.createElement --compress false",
    "clean": "rm -rf ./dist",
    "purge": "rm -rf ./node_modules"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.1"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react-helmet": "5.2.1",
    "rebass": "3.2.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-storysource": "5.3.19",
    "@storybook/react": "5.3.19",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "4.4.1"
  }
}
