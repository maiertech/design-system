{
  "name": "@mdotasia/components",
  "version": "0.0.7",
  "description": "Reusable React components.",
  "homepage": "https://github.com/mdotasia/components",
  "bugs": "https://github.com/mdotasia/components/issues",
  "author": {
    "name": "Thilo Maier",
    "email": "git@maier.asia"
  },
  "repository": "git@github.com:mdotasia/components.git",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "styleguide"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "start-storybook -p 9009",
    "build": "yarn run build:js && yarn run build:storybook",
    "prebuild:js": "rimraf ./dist",
    "build:js": "babel ./src -d ./dist --ignore test.js,stories.jsx,profiles.js",
    "prebuild:storybook": "rimraf ./storybook-static",
    "build:storybook": "build-storybook",
    "format": "pretty-quick",
    "lint": "eslint --ignore-path .gitignore .",
    "precommit": "pretty-quick --staged",
    "prepublishOnly": "yarn run build",
    "release": "release"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "styled-components": "^3.2.6",
    "styled-system": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.5",
    "@storybook/addon-viewport": "^3.4.5",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "release": "^4.0.0",
    "rimraf": "^2.6.2"
  }
}
